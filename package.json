  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/lodash.mapvalues": "^4.6.3",
    "@types/mocha": "^2.2.48",
    "@types/sinon": "^4.1.3",
    "@types/sinon-chai": "^2.7.29",
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "sinon": "^4.3.0",
    "sinon-chai": "^2.14.0",
    "ts-lint": "^4.5.1",
    "ts-node": "^4.1.0",
    "tslint-config-prettier": "^1.7.0",
    "typescript": "^2.7.1"
  },
  "peerDependencies": {
    "sinon": "4.x"
